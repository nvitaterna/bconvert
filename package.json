{
  "name": "bconvert",
  "version": "1.0.0",
  "description": "This is a library to convert to and from any base, including custom character sets. No dependencies.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:nvitaterna/bconvert.git",
  "homepage": "https://github.com/nvitaterna/bconvert",
  "author": "Nicolas Vitaterna <nvitaterna@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "shx rm -rf dist && tsc -p tsconfig.build.json",
    "lint": "eslint --cache --fix src",
    "format": "prettier --write 'src/**/*'",
    "test": "shx rm -rf coverage && yarn jest src",
    "test:single": "yarn jest",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/expect": "^24.3.0",
    "@types/jest": "^28.1.5",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "shx": "^0.3.4",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "convert",
    "number",
    "any",
    "base",
    "base64",
    "base62",
    "base64url",
    "hex",
    "binary",
    "octal"
  ],
  "files": [
    "/dist"
  ],
  "engines": {
    "node": ">=16"
  }
}
